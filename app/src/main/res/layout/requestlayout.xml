<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="330dp"
    android:layout_marginTop="100dp"

    android:background="#fff"
    tools:layout_editor_absoluteX="10dp"
    tools:layout_editor_absoluteY="10dp">


    <TextView
        android:id="@+id/username"
        android:layout_width="175dp"
        android:layout_height="44dp"
        android:layout_x="200dp"
        android:layout_y="31dp"
        android:text="TextView"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="18dp" />

    <TextView
        android:id="@+id/date"
        android:layout_width="182dp"
        android:layout_height="36dp"
        android:layout_x="198dp"
        android:layout_y="76dp"
        android:text="TextView"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="42dp" />

    <TextView
        android:id="@+id/time"
        android:layout_width="181dp"
        android:layout_height="32dp"
        android:layout_x="197dp"
        android:layout_y="129dp"
        android:text="TextView"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/address"
        android:layout_width="173dp"
        android:layout_height="34dp"
        android:layout_x="200dp"
        android:layout_y="169dp"
        android:text="TextView"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="132dp" />

    <TextView
        android:id="@+id/notes"
        android:layout_width="172dp"
        android:layout_height="35dp"
        android:layout_x="201dp"
        android:layout_y="214dp"
        android:text="TextView"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="174dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_x="59dp"
        android:layout_y="275dp"
        android:background="#008577"
        android:text="Approval"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="227dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_x="204dp"
        android:layout_y="273dp"
        android:background="#008577"
        android:text="Rejected"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="223dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="156dp"
        android:layout_height="wrap_content"
        android:layout_x="42dp"
        android:layout_y="23dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="UserName:"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="10dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="156dp"
        android:layout_height="wrap_content"
        android:layout_x="43dp"
        android:layout_y="71dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Date:"
        tools:layout_editor_absoluteX="-30dp"
        tools:layout_editor_absoluteY="68dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_x="43dp"
        android:layout_y="117dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Time:"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="122dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_x="42dp"
        android:layout_y="161dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Address:"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="161dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_x="42dp"
        android:layout_y="208dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Notes:"
        tools:layout_editor_absoluteX="23dp"
        tools:layout_editor_absoluteY="208dp" />
</AbsoluteLayout>